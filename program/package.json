{
  "name": "lesson-manager",
  "version": "1.0.0",
  "author": "Samuel Fernandez",
  "description": "A program to help manage lessons and their standards",
  "private": true,
  "homepage": "./",
  "main": "./build/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "bootstrap": "^5.0.1",
    "concurrently": "^6.2.0",
    "fs": "0.0.1-security",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ebuild": "electron-builder build -w",
    "dev": "electron .",
    "tracewarnings": "electron --trace-warnings",
    "electron-pack": "electron-builder -w"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.fernandez.lessonmanager",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win":{
      "extraFiles": "build/dbs.json",
      "target": "nsis"
    }
  },
  "devDependencies": {
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0"
  }
}
